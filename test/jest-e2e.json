{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": "\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@libs/config/(.*)": "<rootDir>/../libs/config/src/$1",
    "@libs/config": "<rootDir>/../libs/config/src",
    "@libs/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@libs/database": "<rootDir>/../libs/database/src",
    "@libs/redlock/(.*)": "<rootDir>/../libs/redlock/src/$1",
    "@libs/redlock": "<rootDir>/../libs/redlock/src"
  }
}